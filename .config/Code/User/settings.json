{
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.confirmDelete": false,
  "genGetSet.ignoredDictionaryList": ["vscode", "tsconfig"],
  "export-typescript.excludes": ["**/*.{spec.ts,spec.tsx}"],
  "export-typescript.includes": ["**/*.{ts,tsx}", "*/index.{ts,tsx}"],
  "export-typescript.exportStar": true,
  "files.autoSave": "onFocusChange",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "explorer.confirmDragAndDrop": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "files.associations": {
    "**/files/**/*": "plaintext",
    ".gmrc": "jsonc"
  },
  "typescript.tsdk": "node_modules/typescript/lib",
  "cSpell.userWords": [
    "Affjax",
    "Bifunctor",
    "Dotenv",
    "Eagery",
    "Flexbox",
    "Followees",
    "Graphile",
    "Plainionist",
    "Postgres",
    "Queryable",
    "Selda",
    "Semigroup",
    "Serializability",
    "Steyer",
    "Typegoose",
    "bimap",
    "collatz",
    "concat",
    "cqrs",
    "deinit",
    "dtos",
    "esbenp",
    "eval",
    "favorited",
    "foldl",
    "foldr",
    "followee",
    "forall",
    "lmap",
    "marblejs",
    "mariadb",
    "middlewares",
    "mkdir",
    "mongod",
    "mscolnick",
    "nestjs",
    "newtype",
    "ngrx",
    "nrwl",
    "pipeable",
    "postgraphile",
    "prettierignore",
    "prettierrc",
    "purescript",
    "purs",
    "purty",
    "spago",
    "submodule",
    "submodules",
    "typeorm",
    "uncommenting",
    "uncons",
    "unfollow",
    "unformat",
    "unnest",
    "unprocessable",
    "websockets"
  ],
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "markdown.extension.orderedList.marker": "one",
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "editor.cursorBlinking": "solid",
  "editor.formatOnType": false,
  "editor.formatOnPaste": false,
  "editor.minimap.enabled": false,
  "editor.codeActionsOnSave": {
    "source.organizeImports": true,
    "source.addMissingImports": false,
    "source.fixAll": true
  },
  "auto-close-tag.SublimeText3Mode": true,
  "html.autoClosingTags": false,
  "open-file-from-path.regExp": "[\"`]([^`\"]+)['`\"]",
  "open-file-from-path.searchExclusion": [
    "**/node_modules/**",
    "**/.vscode/**",
    "**/dist/**",
    "**/.spago/**",
    "**/.psci_modules/**",
    "**/build/**"
  ],
  "npm-intellisense.showBuildInLibs": true,
  "typescript.preferences.quoteStyle": "single",
  "prettier.singleQuote": true,
  "files.exclude": {
    "**/*.spec.ts": true,
    "**/node_modules": true,
    "**/output": true,
    "**/build": true,
    "**/*.pyc": true,
    "**/__pycache__":true,
  },
  "search.exclude": {
    ".spago": true,
    "**/.cache": true,
    "yarn.lock": true
  },
  "git.enabled": false,
  "git.path": null,
  "git.autofetch": false,
  "sendtoterminalplus.languages": [
    {
      "langId": "default",
      "delayMode": "default",
      "payloadFormat": "default",
      "linePattern": "{line}",
      "noSelectionPayload": ["{currentline}"],
      "oneLineSelectionPayload": ["{selection}"],
      "multiLineSelectionPayload": ["{selection}"]
    },
    {
      "langId": "purescript",
      "delayMode": "default",
      "payloadFormat": "chunk",
      "linePattern": "{line}",
      "noSelectionPayload": ["{currentline}"],
      "oneLineSelectionPayload": ["{selection}"],
      "multiLineSelectionPayload": [":paste", "{selection}", "\u0004"]
    },
    {
      "langId": "haskell",
      "delayMode": "default",
      "payloadFormat": "chunk",
      "linePattern": "{line}",
      "noSelectionPayload": ["{currentline}"],
      "oneLineSelectionPayload": ["{selection}"],
      "multiLineSelectionPayload": [":{", "{selection}", ":}"]
    },
    {
      "langId": "python",
      "delayMode": "default",
      "payloadFormat": "chunk",
      "linePattern": "{line}",
      "noSelectionPayload": ["{currentline}"],
      "oneLineSelectionPayload": ["{selection}"],
      "multiLineSelectionPayload": ["{selection}", ""]
    }
  ],
  "cSpell.enabledLanguageIds": [
    "asciidoc",
    "c",
    "cpp",
    "csharp",
    "css",
    "git-commit",
    "go",
    "graphql",
    "handlebars",
    "haskell",
    "html",
    "jade",
    "java",
    "javascript",
    "javascriptreact",
    "json",
    "jsonc",
    "latex",
    "less",
    "markdown",
    "php",
    "plaintext",
    "python",
    "pug",
    "restructuredtext",
    "rust",
    "scala",
    "scss",
    "text",
    "typescript",
    "typescriptreact",
    "yaml",
    "yml",
    "purescript"
  ],
  "cSpell.enableFiletypes": ["!typescript"],
  "files.watcherExclude": {
    "**/.output/**": true,
    "**/.spago/**": true
  },
  "window.zoomLevel": 0,
  "sqltools.queryParams.enableReplace": true,
  "sql-formatter.uppercase": true,
  "pgFormatter.spaces": 2,
  "vscode-mysql.enableDelimiter": true,
  "search.searchOnType": false,
  "database-client.disableSqlCodeLen": true,
  "macros.list": {
    "deletePrefix": ["deleteLeft", "deleteLeft", "deleteLeft", "deleteLeft"]
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "brittany.path": "/home/a/.local/bin/brittany",
  "haskell.hlint.executablePath": "/home/a/.local/bin/hlint",
  "[haskell]": {
    "editor.defaultFormatter": "MaxGabriel.brittany"
  },
  "terminal.integrated.inheritEnv": true,
  "[json]": {
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.suggest.insertMode": "replace"
  },
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "extensions.ignoreRecommendations": true,
  "rainbow_csv.autodetect_separators": ["\t", ";", "|"],
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "editor.wordBasedSuggestions": false,
  "editor.suggest.insertMode": "replace",
  "editor.semanticHighlighting.enabled": true
}
